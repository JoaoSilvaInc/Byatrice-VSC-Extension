{
  "publisher": "JoaoSilva",
  "repository":  {
    "type": "git",
    "url": "https://github.com/JoaoSilvaInc/Byatrice-VSC-Extension.git"
  },
  "name": "byatrice-extension",
  "displayName": "BYTC File Viewer",
  "description": "View .bytc files with readable function names",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "byatrice-extension.openBytcFile",
        "title": "Open .bytc File"
      }
    ],
    "languages": [
      {
        "id": "bytc",
        "aliases": [
          "BYTC",
          "bytc"
        ],
        "extensions": [
          ".bytc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bytcExplorer",
          "title": "BYTC",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "bytcExplorer": [
        {
          "id": "bytcView",
          "name": "BYTC View"
        }
      ]
    },
    "fileExplorer": {
      "command": "byatrice-extension.openBytcFile"
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
